          },
          "hide": false,
          "query": "coords = from(bucket: \"bike_monitoring\")\r\n  |> range(start: v.timeRangeStart)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"station_knowledge\")\r\n  |> last()\r\n\r\ndata = from(bucket: \"bike_monitoring\")\r\n  |> range(start: v.timeRangeStart)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"station\")\r\n  |> last()\r\n\r\nunion(tables: [coords, data])\r\n  |> map(fn: (r) => ({ r with ID: r.station_id })) \r\n  |> drop(columns: [\"station_id\", \"_start\", \"_stop\", \"_measurement\", \"_time\"]) \r\n  |> pivot(rowKey:[\"ID\"], columnKey: [\"_field\"], valueColumn: \"_value\")",
